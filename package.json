{
  "name": "mhcms",
  "version": "0.1.1",
  "description": "Minimal Headless Content Management System",
  "//": "Overrides for node 14.17 and above",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/types/index.d.ts",
  "files": [
    "./dist/**/*"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./dist/esm/index.js",
        "types": "./dist/esm/types/index.d.ts"
      },
      "require": {
        "default": "./dist/cjs/index.js",
        "types": "./dist/cjs/types/index.d.ts"
      }
    },
    "./file-access": {
      "import": {
        "default": "./dist/esm/file-access/index.js",
        "types": "./dist/esm/types/file-access/index.d.ts"
      },
      "require": {
        "default": "./dist/cjs/file-access/index.js",
        "types": "./dist/cjs/types/file-access/index.d.ts"
      }
    }
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "prepare": "npm run build",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "test": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trouve-antoine/mhcms.git"
  },
  "keywords": [
    "cms"
  ],
  "author": "Antoine Trouv√©",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trouve-antoine/mhcms/issues"
  },
  "homepage": "https://github.com/trouve-antoine/mhcms#readme",
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.9",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.388.0",
    "fp-ts": "^2.16.1",
    "io-ts": "^2.2.20",
    "yaml": "^2.3.1"
  }
}
